{
  "mappings": [
    {
      "name": "Gets SupplierProject by search criteria: NE3 and AreaProgram with and id = 10 (always returned by previous mapping)",
      "request": {
        "method": "POST",
        "urlPattern": "/giga-project-dioss/v2/supplierprojects/search",
        "bodyPatterns": [
          {
            "matchesJsonPath": "[?(@.areaProgramBid == 10)][?(@.ne3 == true)]"
          }
        ]
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "bodyFileName": "program-management/supplier-project-for-area-program-10-ne3.json"
      }
    },
