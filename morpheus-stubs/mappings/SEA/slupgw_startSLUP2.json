{
	"id": "0b02bb3f-62c1-4c3d-bced-df3ac43b53c9",
	"request": {
		"urlPattern": "/slupService",
		"method": "POST",
		"headers": {
			"Content-Type": {
				"contains": "text/xml"
			},
			"SOAPAction": {
				"contains": "/slupService/startSLUP2"
			}
		},
		"bodyPatterns": [
			{
				"matchesXPath": "//slup:startSLUP2/slupURL",
				"xPathNamespaces": {
					"slup": "http://slup2soap.idm.telekom.com/slupService/"
				}
			}
		]
	},
	"response": {
		"status": 200,
		"headers": {
			"Content-Type": "text/xml"
		},
		"fixedDelayMilliseconds": 1000,
		"body": "<soapenv:Envelope xmlns:soapenv='http://schemas.xmlsoap.org/soap/envelope/' xmlns:slup='http://slup2soap.idm.telekom.com/slupService/'><soapenv:Header/><soapenv:Body><slup:startSLUP2Response><SLUPreturncode>0000</SLUPreturncode><detail>OK, SLUP Queue was started</detail><token>12345678</token></slup:startSLUP2Response></soapenv:Body></soapenv:Envelope>"
	},
	"postServeActions": {
		"webhooks": {
			"webhooks": [
				{
					"method": "POST",
					"fixedDelayMilliseconds": 3000,
					"headers": {
						"Authorization": "Bearer {{oauth}}",
						"Content-Type": "text/xml"
					},
					"url": "{{soapXPath request.body '/startSLUP2/slupURL/text()'}}",
					"body": "<soapenv:Envelope xmlns:soapenv='http://schemas.xmlsoap.org/soap/envelope/' xmlns:slup='http://slup2soap.idm.telekom.com/slupClient/'><soapenv:Header/><soapenv:Body><slup:SLUPConnect><token>12345678</token></slup:SLUPConnect></soapenv:Body></soapenv:Envelope>"
				}
			]
		}
	}
}