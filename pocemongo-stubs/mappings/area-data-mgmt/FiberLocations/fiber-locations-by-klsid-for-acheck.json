{
  "priority": 5,
  "request": {
    "method": "GET",
    "urlPath": "/giga-areas-location/v2/fibreOnLocations/search",
    "queryParameters": {
      "klsIds": {
        "matches": "30000000[3-8]"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "area-data-mgmt/FiberLocations/fiber-locations-by-klsid-{{request.query.klsIds}}-payload.json"
  }
}