{
  "request": {
    "method": "GET",
    "urlPattern": "/resourceInventory/v1/ipRangeAssignments/{{now format='yyMMddHHmmss'}}"
  },
  "priority": 5,
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "id": {{now format='yyMMddHHmmss'}},
      "@type": "IpRangeAssignment",
      "href": "http://{{hostname}}/resourceInventory/v1/ipRangeAssignments/{{now format='yyMMddHHmmss'}}",
      "ipRangePool": "{{jsonPath request.body '$.ipRangePool'}}",
      "parentInterface": {
      "endSz": "{{jsonPath request.body '$.parentInterface.endSz'}}",
      "port": "{{jsonPath request.body '$.parentInterface.port'}}"
      },
      "ipAddress": "10.150.240.100",
      "prefixLength": 30
    }
  }
}