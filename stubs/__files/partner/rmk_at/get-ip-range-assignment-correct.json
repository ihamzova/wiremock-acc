{
  "request": {
    "method": "GET",
    "urlPattern": "/resourceInventory/v1/ipRangeAssignments/(\\d*)"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "id": {{request.requestLine.pathSegments.[3]}},
      "@type": "IpRangeAssignment",
      "href": "http://{{hostname}}/resourceInventory/v1/ipRangeAssignments/{{request.requestLine.pathSegments.[3]}}",
      "ipRangePool": "ancp_olt",
      "parentInterface": {
      "endSz": "49/30/179/43G1",
      "port": "1"
      },
      "ipAddress": "10.150.240.100",
      "prefixLength": 30
    }
  }
}