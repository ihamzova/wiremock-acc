{
  "request": {
    "method": "POST",
    "urlPattern": "/Wholebuy/v1/resourceOrder",
    "headers": {}
  },
  "response": {
    "status": 202,
    "statusMessage": "Async response. Reservation request is accepted.",
    "headers": {
      "Content-Type": "application/json",
      "X-Callback-Correlation-Id": "{{request.headers.X-Callback-Correlation-Id}}"
    }
  },
  "postServeActions": {
    "webhooks": {
      "webhooks": [
        {
          "method": "POST",
          "fixedDelayMilliseconds": 6000,
          "headers": {
            "Authorization": "Bearer {{oauth}}",
            "Content-Type": "application/json",
            "X-Callback-Correlation-Id": "{{request.headers.X-Callback-Correlation-Id}}"
          },
          "url": "https://{{systemValue type='ENVIRONMENT' key='API_GATEWAY_URL'}}/pubsub/p/Wholebuy/v1/WholebuyResourceOrder/Events",
          "bodyFileName": "partner/congstar/resource-order/resource-order-update.json"
        },
        {
          "method": "POST",
          "fixedDelayMilliseconds": 6000,
          "headers": {
            "Authorization": "Bearer {{oauth}}",
            "Content-Type": "application/json",
            "X-Callback-Correlation-Id": "{{request.headers.X-Callback-Correlation-Id}}"
          },
          "url": "{{request.headers.X-Callback-Url}}",
          "bodyFileName": "partner/congstar/resource-order/resource-order-complete.json"
        }
      ]
    }
  }
}