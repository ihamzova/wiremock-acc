{
  "request": {
    "method": "POST",
    "urlPattern": "/FlexKommCWFService/",
    "headers": {
      "X-B3-SpanId": {
        "matches": "[0-9A-Za-z]{16}"
      },
      "X-B3-Sampled": {
        "matches": "[0-1]"
      },
      "X-B3-TraceId": {
        "matches": "[0-9A-Za-z]{16}"
      },
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "contains": "sendeEingangsbestaetigung"
      }
    ]
  },
  "response": {
    "status": 201
  }
}
