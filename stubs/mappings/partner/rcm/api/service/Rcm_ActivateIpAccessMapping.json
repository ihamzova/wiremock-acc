{
  "request": {
    "method": "POST",
    "urlPath": "/reqcb/service-order-service-inventory/v1/rcm-service",
	"bodyPatterns" : [ {
      "matchesJsonPath" : "$.[?(@.name=='IpAccessConfigOperational')]"
    } ]
  },


  "response": {
    "status": 202,
    "statusMessage": "Async response. Reservation request is accepted.",
	"headers": {
		"Content-Type": "application/json",
        "X-Callback-Correlation-Id": "{{jsonPath request.body '$.id'}}"
	}
  },
  
   "postServeActions": {
    "webhook": {
      "method": "POST",
 	  "fixedDelayMilliseconds": 5000,
      "headers": {
        "Content-Type": "application/json",
        "X-Callback-Correlation-Id": "{{jsonPath request.body '$.id'}}"
      },
      "url": "{{request.headers.X-Callback-Url}}",
	  "bodyFileName": "partner/rcm/Rcm_ActivateIpAccess.json"
    }
  }
}