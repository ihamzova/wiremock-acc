{
  "priority": 4,
  "request": {
    "method": "GET",
    "urlPath": "/resource-order-resource-inventory/v1/a4NetworkElements",
    "queryParameters": {
      "vpsz": {
        "matches": "[0-9]{1,6}\\/[0-9]{1,6}\\/[0-9]{1,6}"
      },
      "fsz": {
        "matches": "7KH4|7KH0"
      },
      "category": {
        "equalTo": "OLT"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "rori/a4-resource-inventory/networkelements/search-network-elements-by-vpsz-fsz-{{replace request.query.vpsz '/' '_'}}_{{request.query.fsz}}.json"
  }
}
