{
  "priority": 5,
  "request": {
    "method": "POST",
    "urlPath": "/accessLineResourceInventory/v2/accessLine/search",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.homeId =~ /[A-Z0-9]{7}/)]"
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "rori/access-line-resource-inventory/search-access-line-by-homeId-{{jsonPath request.body '$.homeId'}}.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}