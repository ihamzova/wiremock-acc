{
  "priority": 2,
  "request": {
    "method": "GET",
    "urlPath": "/api/oltResourceInventory/v1/card",
    "queryParameters": {
      "endSz": {
        "equalTo": "49/30/179/76H3"
      },
      "slotNumber": {
        "matches": "3|4"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "rori/olt-resource-inventory/card/{{replace request.query.endSz '/' '_'}}/slot_{{request.query.slotNumber}}.json"
  }
}
