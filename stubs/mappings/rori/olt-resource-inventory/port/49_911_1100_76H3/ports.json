{
  "priority": 2,
  "request": {
    "method": "GET",
    "urlPath": "/api/oltResourceInventory/v1/port",
    "queryParameters": {
      "endSz": {
        "equalTo": "49/911/1100/76H3"
      },
      "portNumber": {
        "matches": "[0-9]|[1][0-5]{1}"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "rori/olt-resource-inventory/port/{{replace request.query.endSz '/' '_'}}/{{request.query.portNumber}}.json"
  }
}
