{
  "priority": 1,
  "request": {
    "method": "DELETE",
    "urlPathPattern": "/service-order-service-inventory/v1/(ont|testOnt)(E6[A-F0-9]{14})/async",
    "headers": {
      "X-Callback-Correlation-Id": {
        "matches": "[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}"
      }
    }
  },
  "response": {
    "status": 202,
    "statusMessage": "Async response. Reservation request is accepted."
  },
  "postServeActions": {
    "webhook": {
      "method": "POST",
      "headers": {
        "Content-Type": "application/json",
        "X-Callback-Correlation-Id": "{{request.headers.X-Callback-Correlation-Id}}"
      },
      "jsonBody": {
        "somePayload": {
          "someNumber": 123,
          "someString": "123"
        }
      },
      "url": "{{request.headers.X-Callback-Error-Url}}"
    }
  }
}